#include <stdio.h>
int main()
{
int array[10], search, i, n;
for (i = 0; i < 5; i++)
{
printf("Enter a number\n : ");
scanf("%d", &array[i]);
scanf("%d", &search);
}
printf("Enter the number to be searched : %d\n",search);
for (i = 0; i < 5; i++)
{
if (array[i] == search)
{

printf("The number %d appears in the first five numbers\n", search);
break;
}
}
if (i == 5)
printf("The number %d does not appears in the first five numbers", search);
return 0;
}
