#include<stdio.h>
int main()
{
int n,i,flag=0;
printf("\n enter the n value");
scanf("%d",&n);
for(i=2;i<=(n/2);++i)
{
if(n%i==0)
{
flag=1;
break;
}
}
if(n==1)
{
 printf("\n it is a neither prime nor composite nuamber");
}
else
if(flag==0)
{
printf("\n prime number");
}
else
printf("\n not a prime number");
return 0;
}
